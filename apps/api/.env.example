# Database
DATABASE_URL=postgresql://user:password@host:5432/respire
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_JWT_SECRET=your-jwt-secret

# Application URL (for email redirects)
APP_URL=https://app.tryrespire.ai

# Redis
REDIS_URL=redis://default:password@host:6379

# WHOOP API
WHOOP_CLIENT_ID=your-client-id
WHOOP_CLIENT_SECRET=your-client-secret
WHOOP_REDIRECT_URI=https://app.tryrespire.ai/settings/whoop/callback

# CORS Allowed Origins (comma-separated)
ALLOWED_ORIGINS=https://tryrespire.ai,https://app.tryrespire.ai,https://www.tryrespire.ai

# OpenAI
OPENAI_API_KEY=sk-your-key

# JWT
SECRET_KEY=your-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Environment
ENVIRONMENT=development